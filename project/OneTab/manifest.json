{
    "manifest_version": 2,
    "name": "OneTab",
    "description": "only one tab opened with same domain",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "app": {
        "urls": [
            "http://certification.googleapps.com/app-info/"
        ],
        "launch": {
            "web_url": "http://certification.googleapps.com/"
        }
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "webRequest"
    ],
    "options_page": "options.html",
    "background": {
        "service_worker": "daemon.js"
    }
}