{
    "manifest_version": 3,
    "name": "OneTab",
    "description": "only one tab opened with same domain",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "webRequest"
    ],
    "options_page": "options.html",
    "host_permissions": ["*://*/*"],
    "background": {
        "service_worker": "daemon.js"
    }
}